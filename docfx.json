{
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "**.csproj"
          ]
        }
      ],
      "dest": "docs/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net9.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**.{md,yml}"
        ],
        "src": "docs"
      },
      {
        "files": [
          "**.md"
        ],
        "src": ".",
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          ".nojekyll"
        ],
        "src": "docs"
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "TuneBridge",
      "_appName": "TuneBridge API Documentation",
      "_appFooter": "Copyright Â© 2025 Taylor Marvin",
      "_enableSearch": true
    }
  }
}
